# simple inputs files for the unsplit CTU hydro scheme

[driver]
max_steps = 500
tmax = 0.1

max_dt_change = 1.e33
#init_tstep_factor = 1.0


[driver]
cfl = 0.8

[io]
basename = compare_
dt_out = 0.2

[mesh]
nx = 128
ny = 16
xmax = 1.0
ymax = 0.125

xlboundary = outflow
xrboundary = outflow

ylboundary = periodic
yrboundary = periodic


[burgers]
limiter = 2
