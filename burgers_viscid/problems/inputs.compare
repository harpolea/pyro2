# simple inputs files for the unsplit CTU hydro scheme

[driver]
max_steps = 500
tmax = 2

max_dt_change = 1.e33
init_tstep_factor = 1.0
fix_dt = 0.1


[driver]
cfl = 0.8

[io]
basename = visc_compare_
dt_out = 10000000

[mesh]
nx = 64
ny = 64
xmax = 1.0
ymax = 1.0

xlboundary = constant
xrboundary = constant

ylboundary = periodic
yrboundary = periodic


[burgers]
limiter = 2
