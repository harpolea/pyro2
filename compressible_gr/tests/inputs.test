# simple inputs files for the four-corner problem.

[driver]
max_steps = 10
tmax = 10000000000.0


[io]
basename = ../../Documents/Work/pyro/results/test/test_4_
n_out = 1                   ; number of timesteps between writing output files
do_io = 1                   ; do we output at all?


[mesh]
nx = 4
ny = 4
xmax = 4.0
ymax = 4.0

xlboundary = outflow
xrboundary = outflow

ylboundary = outflow
yrboundary = outflow


[test]
dens_base = 10.0

[compressible-gr]
react = 1
