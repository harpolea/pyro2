# simple inputs files for the four-corner problem.

[driver]
max_steps = 1000
tmax = 10000000000.0
verbose = 0


[io]
basename = ../../Documents/Work/pyro/results/test/test_4_
n_out = 1                   ; number of timesteps between writing output files
do_io = 0                   ; do we output at all?


[mesh]
nx = 16
ny = 16
xmax = 4.0
ymax = 4.0

xlboundary = outflow
xrboundary = outflow

ylboundary = outflow
yrboundary = outflow


[test]
dens_base = 10.0

[compressible-gr]
react = 1
grav = 0.0                ; gravitational acceleration (in y-direction)
